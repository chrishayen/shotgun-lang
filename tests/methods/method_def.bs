// Test: Method definition with self
Person :: struct {
    name str
    age int
}

Person :: greet(self) str {
    return "Hello, I am {self.name}"
}

Person :: is_adult(self) bool {
    if self.age >= 18 {
        return true
    }
    return false
}

fn main() {
    Person p = Person { name: "Alice", age: 30 }
    print("Method defined")
}
