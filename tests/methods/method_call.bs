// Test: Method invocation
Counter :: struct {
    value int
}

Counter :: get(self) int {
    return self.value
}

Counter :: increment(self) int {
    return self.value + 1
}

fn test_method_get() {
    Counter c = Counter { value: 10 }
    int v = c.get()
    assert_eq(v, 10)
}

fn test_method_increment() {
    Counter c = Counter { value: 10 }
    int next = c.increment()
    assert_eq(next, 11)
}
