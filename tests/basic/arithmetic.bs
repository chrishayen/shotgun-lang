// Test: Arithmetic operations
fn main {
    int a = 10
    int b = 3

    int sum = a + b
    int diff = a - b
    int prod = a * b
    int quot = a / b
    int rem = a % b

    print("Sum: {sum}")
    print("Diff: {diff}")
    print("Prod: {prod}")
    print("Quot: {quot}")
    print("Rem: {rem}")
}
