// Test: Arithmetic operations

fn test_addition() {
    int a = 10
    int b = 3
    int sum = a + b
    assert_eq(sum, 13)
}

fn test_subtraction() {
    int a = 10
    int b = 3
    int diff = a - b
    assert_eq(diff, 7)
}

fn test_multiplication() {
    int a = 10
    int b = 3
    int prod = a * b
    assert_eq(prod, 30)
}

fn test_division() {
    int a = 10
    int b = 3
    int quot = a / b
    assert_eq(quot, 3)
}

fn test_modulo() {
    int a = 10
    int b = 3
    int rem = a % b
    assert_eq(rem, 1)
}
