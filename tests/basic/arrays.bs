// Test: Array literals and operations

fn test_int_array_access() {
    int[] numbers = [1, 2, 3, 4, 5]
    assert_eq(numbers[0], 1)
    assert_eq(numbers[2], 3)
    assert_eq(numbers[4], 5)
}

fn test_int_array_multiple() {
    int[] nums = [10, 20, 30]
    int first = nums[0]
    int second = nums[1]
    int third = nums[2]
    assert_eq(first, 10)
    assert_eq(second, 20)
    assert_eq(third, 30)
}
