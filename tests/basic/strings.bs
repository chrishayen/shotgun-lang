// Test: String literals
// Note: String interpolation comparison requires strcmp (not yet implemented)
// For now, we test simple string variable assignment

fn test_simple_string() {
    str simple = "Hello"
    assert_eq(simple, "Hello")
}

fn test_string_variable() {
    str name = "World"
    assert_eq(name, "World")
}

fn test_string_literal_equality() {
    str a = "test"
    str b = "test"
    assert_eq(a, b)
}
