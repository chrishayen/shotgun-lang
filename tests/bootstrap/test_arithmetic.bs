// Arithmetic tests

fn main {
    t := new_test()

    // Basic arithmetic
    t.assert_eq_int(5, 2 + 3)
    t.assert_eq_int(7, 10 - 3)
    t.assert_eq_int(12, 3 * 4)
    t.assert_eq_int(5, 15 / 3)
    t.assert_eq_int(1, 10 % 3)

    // Operator precedence
    t.assert_eq_int(14, 2 + 3 * 4)
    t.assert_eq_int(20, (2 + 3) * 4)
    t.assert_eq_int(11, 3 + 16 / 2)

    // Negative numbers (using subtraction)
    int neg = 0 - 5
    t.assert_eq_int(0 - 5, neg)
    t.assert_eq_int(5, 0 - neg)

    // Compound operations
    int x = 10
    x += 5
    t.assert_eq_int(15, x)

    x -= 3
    t.assert_eq_int(12, x)

    x *= 2
    t.assert_eq_int(24, x)

    x /= 4
    t.assert_eq_int(6, x)

    // Comparison operators
    t.assert_true(5 == 5)
    t.assert_false(5 == 6)
    t.assert_true(5 != 6)
    t.assert_false(5 != 5)

    t.assert_true(3 < 5)
    t.assert_false(5 < 3)
    t.assert_false(5 < 5)

    t.assert_true(5 > 3)
    t.assert_false(3 > 5)
    t.assert_false(5 > 5)

    t.assert_true(5 <= 5)
    t.assert_true(3 <= 5)
    t.assert_false(6 <= 5)

    t.assert_true(5 >= 5)
    t.assert_true(5 >= 3)
    t.assert_false(3 >= 5)

    // Integer division
    t.assert_eq_int(3, 10 / 3)
    t.assert_eq_int(0, 1 / 2)
    t.assert_eq_int(1, 5 / 5)

    // Modulo
    t.assert_eq_int(0, 10 % 5)
    t.assert_eq_int(2, 17 % 5)
    t.assert_eq_int(0, 0 % 7)

    t.summary()
}
