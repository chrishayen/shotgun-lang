// Test: Generic struct definition && usage

Box<T> :: struct {
    value T
}

fn main() {
    Box<int> int_box = Box<int> { value: 42 }
    Box<str> str_box = Box<str> { value: "hello" }

    int n = int_box.value
    str s = str_box.value

    if n == 42 && s == "hello" {
        print("generic struct test passed")
    } else {
        print("FAIL: values don't match")
    }
}
