// Test: Methods on generic structs with additional parameters

Box<T> :: struct {
    value T
}

Box<T> :: add(self, T other) T {
    return self.value + other
}

Box<T> :: get(self) T {
    return self.value
}

fn main() {
    Box<int> b = Box<int> { value: 10 }
    int sum = b.add(32)
    int x = b.get()

    if sum == 42 && x == 10 {
        print("generic method with params test passed")
    } else {
        print("FAIL: method results don't match")
    }
}
