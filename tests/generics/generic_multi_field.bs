// Test: Generic struct with multiple fields using type param

Container<T> :: struct {
    first T
    second T
    count int
}

fn main {
    Container<int> c = Container<int> {
        first: 10,
        second: 20,
        count: 2
    }

    int a = c.first
    int b = c.second
    int n = c.count
    int sum = a + b

    if a == 10 && b == 20 && n == 2 && sum == 30 {
        print("generic multi field test passed")
    } else {
        print("FAIL: field values incorrect")
    }
}
