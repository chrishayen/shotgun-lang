// Test: Multiple variables of same generic instantiation

Box<T> :: struct {
    value T
}

fn main {
    Box<int> a = Box<int> { value: 1 }
    Box<int> b = Box<int> { value: 2 }
    Box<int> c = Box<int> { value: 3 }

    int sum = a.value + b.value + c.value

    if sum == 6 {
        print("generic reuse same test passed")
    } else {
        print("FAIL: sum doesn't match")
    }
}
