// Test: Methods on generic structs

Box<T> :: struct {
    value T
}

Box<T> :: get(self) T {
    return self.value
}

fn main {
    Box<int> b = Box<int> { value: 42 }
    int x = b.get()

    if x == 42 {
        print("generic method test passed")
    } else {
        print("FAIL: method returned wrong value")
    }
}
