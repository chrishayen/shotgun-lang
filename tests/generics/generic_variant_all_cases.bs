// Test: Match on all variant cases

Result<T, E> :: variant {
    Ok { value T }
    Err { err E }
}

fn main() {
    Result<int, str> ok = Result<int, str>.Ok { value: 42 }
    Result<int, str> fail = Result<int, str>.Err { err: "something went wrong" }

    str x = match ok using Result<int, str> {
        Ok -> { "ok" }
        Err -> { "err" }
    }

    str y = match fail using Result<int, str> {
        Ok -> { "ok" }
        Err -> { "err" }
    }

    if x == "ok" && y == "err" {
        print("generic variant all cases test passed")
    } else {
        print("FAIL: match results incorrect")
    }
}
