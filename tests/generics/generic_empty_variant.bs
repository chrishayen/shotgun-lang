// Test: Generic variant with empty case (no fields)

Option<T> :: variant {
    Some { value T }
    None
}

fn main() {
    Option<int> empty = Option<int>.None
    Option<int> filled = Option<int>.Some { value: 42 }

    str result1 = match empty using Option<int> {
        Some -> { "some" }
        None -> { "none" }
    }

    str result2 = match filled using Option<int> {
        Some -> { "some" }
        None -> { "none" }
    }

    if result1 == "none" && result2 == "some" {
        print("generic empty variant test passed")
    } else {
        print("FAIL: match results incorrect")
    }
}
