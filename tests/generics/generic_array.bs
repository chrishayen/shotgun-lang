// Test: Arrays of generic types

Box<T> :: struct {
    value T
}

fn main {
    Box<int>[] boxes = [
        Box<int> { value: 1 },
        Box<int> { value: 2 },
        Box<int> { value: 3 }
    ]

    int sum = 0
    for b in boxes {
        sum = sum + b.value
    }

    if sum == 6 {
        print("generic array test passed")
    } else {
        print("FAIL: sum should be 6")
    }
}
