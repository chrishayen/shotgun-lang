// Test: Accessing fields on generic struct instances

Box<T> :: struct {
    value T
}

Pair<A, B> :: struct {
    first A
    second B
}

fn main {
    Box<int> b = Box<int> { value: 42 }
    int x = b.value

    Pair<int, str> p = Pair<int, str> { first: 10, second: "hello" }
    int n = p.first
    str s = p.second

    if x == 42 and n == 10 and s == "hello" {
        print("generic field access test passed")
    } else {
        print("FAIL: field values incorrect")
    }
}
